# Application
NODE_ENV=production
PORT=3000

# PostgreSQL Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=customer_pannel
POSTGRES_PORT=5433

# Database Connection URL (used by Prisma)
# Note: Use localhost:5433 when connecting from host machine
# Inside Docker, containers communicate via service name on port 5432
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/customer_pannel?schema=public
